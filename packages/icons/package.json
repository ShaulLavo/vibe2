{
	"name": "@repo/icons",
	"version": "0.0.0",
	"private": true,
	"exports": {
		".": "./src/index.ts",
		"./lib": "./src/lib/index.tsx",
		"./vs": {
			"types": "./dist/vs/index.d.ts",
			"import": "./dist/vs/index.js",
			"require": "./dist/vs/index.cjs",
			"default": "./dist/vs/index.js"
		},
		"./vs/*": {
			"types": "./dist/vs/*.d.ts",
			"import": "./dist/vs/*.js",
			"require": "./dist/vs/*.cjs",
			"default": "./dist/vs/*.js"
		}
	},
	"scripts": {
		"prebuild": "bun ./scripts/fetch-packs.ts",
		"lint": "eslint . --max-warnings 0",
		"check-types": "tsc --noEmit",
		"build": "bun ./src/build/index.ts",
		"fetch-packs": "bun ./scripts/fetch-packs.ts"
	},
	"dependencies": {
		"solid-js": "^1.9.5"
	},
	"devDependencies": {
		"@repo/eslint-config": "*",
		"@repo/logger": "*",
		"@repo/typescript-config": "*",
		"@types/node": "^24.10.1",
		"cheerio": "^1.0.0-rc.12",
		"svgo": "^3.3.2",
		"eslint": "^9.39.1",
		"typescript": "5.9.2",
		"zod": "^4.1.13"
	}
}
